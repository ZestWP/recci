<h1>Listing interesteds</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interesteds.each do |interested| %>
      <tr>
        <td><%= interested.title %></td>
        <td><%= link_to 'Show', interested %></td>
        <td><%= link_to 'Edit', edit_interested_path(interested) %></td>
        <td><%= link_to 'Destroy', interested, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Interested', new_interested_path %>
